import numpy as np

from skimage.io import imread

import matplotlib.pyplot as plt
import math as m

mandrill = imread("mandrill.png")

def norm(img):
    img = img/255

def RGBtoCMY(img):
    neu = np.zeros(img.shape)
    for i in range(img.shape[0]): #Row
        for j in range(img.shape[1]): #column
            c = (255 - img[0,1][0]) / 255
            m = (255 - img[0,1][1]) / 255
            y = (255 - img[0,1][2]) / 255
            neu[i,j] = c,m,y
            
    return neu

plt.imshow(RGBtoCMY(mandrill))           
            
            
            
    
    
